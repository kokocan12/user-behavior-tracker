    "chalk": "^4.1.1",
    "cypress": "^9.5.4",
    "elapsed-time-logger": "^1.1.7",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "exec-sh": "^0.4.0",
    "lint-staged": "^12.3.7",
